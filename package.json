{
  "name": "pigNews",
  "version": "1.0.0",
  "description": "小猪新闻（H5版）:小猪新闻提供滚动资讯,高清图集,视频集锦等新闻信息",
  "keywords": [
    "小猪新闻",
    "滚动资讯",
    "高清图集",
    "视频集锦"
  ],
  "author": "ldan@wibetter",
  "license": "MIT",
  "scripts": {
    "dev": "akfun dev --env=local",
    "build": "akfun build --env=online",
    "lint": "eslint --ext .js,.vue src",
    "lintFix": "eslint --ext .js,.vue src --fix",
    "format": "prettier --write \"src/**/**/*.{js,vue,tsx,ts,scss}\""
  },
  "files": [
    "dist/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+http://git@github.com:wibetter/pigNews.git"
  },
  "bugs": {
    "url": "https://github.com/wibetter/pigNews/issues"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "@better-scroll/core": "^2.0.0-beta.4",
    "core-js": "^2.6.5",
    "jsonp": "^0.2.1",
    "md5": "^2.2.1",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "akfun": "^1.1.12",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 8",
    "Firefox >= 20",
    "Android > 4.4"
  ]
}
